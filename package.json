{
  "name": "express-sparql",
  "version": "0.1.0",
  "description": "Easy SPARQL server with express.js",
  "main": "lib/index.js",
  "author": "Steffan Sluis <steffansluis@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf lib",
    "prestart": "npm run build",
    "start": "node lib",
    "pretest": "npm run clean && tsc --inlineSourceMap",
    "test": "nyc ava",
    "watch-test": "nodemon -w test -x 'npm run test'",
    "coverage": "codecov",
    "watch": "tsc --watch & nodemon; wait"
  },
  "nyc": {},
  "ava": {
    "files": [
      "test/**/*.js"
    ],
    "source": [
      "src/**/*.ts"
    ],
    "match": [],
    "concurrency": 5,
    "failFast": true,
    "failWithoutAssertions": false,
    "tap": false,
    "powerAssert": false,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "dependencies": {
    "@types/express": "^4.16.0",
    "@types/node": "^10.5.2",
    "express": "^4.16.3",
    "typescript": "^2.9.2"
  },
  "devDependencies": {
    "ava": "^0.20.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "codecov": "^1.0.1",
    "nodemon": "^1.12.1",
    "nyc": "^11.1.0"
  }
}
